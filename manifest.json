{
    "name": "Whatsapp darkmode enabler",
    "version": "1.1",
    "description": "Enables the (currently in beta) native dark mode feature in Whatsapp web",
    "manifest_version": 2,
    "icons": {
        "48": "48.png",
        "128": "128.png",
        "256": "256.png"
    },
    "content_scripts": [{
        "js": ["classupdater.js"],
        "matches": ["https://web.whatsapp.com/*"]
    }]
}
